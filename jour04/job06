M = [1, 2, 3, 4, 5]
print(M)
M[0] , M[-1] = M[-1] , M[0]
print(M)


#______________________________2manière_________

def inverser(liste):
    print(liste)
    liste[0], liste[-1] = liste[-1], liste[0]
    print(liste)
    return liste

# Utilisation de la fonction avec la liste donnée
ma_liste = [1, 2, 3, 4, 5]
resultat = inverser(ma_liste)

# Affichage du résultat
print(resultat)
